-include= jar:${fileuri;${repo;org.apache.ws.security:wss4j;1.6.18}}!/META-INF/MANIFEST.MF,bnd.overrides

wss4jVersion=1.6.18

-includeresource: \
  @${repo;org.apache.ws.security:wss4j;${wss4jVersion}}!/!META-INF/maven/*, \
  @${repo;org.apache.ws.security:wss4j;${wss4jVersion}}!/META-INF/**,\
  @${repo;org.apache.ws.security:wss4j;${wss4jVersion}}!/templates/**,\
  @${repo;org.apache.ws.security:wss4j;${wss4jVersion}}!/schemas/**,\
  @${repo;org.apache.ws.security:wss4j;${wss4jVersion}}!/*xml,\
  org/apache/ws/security=${bin}/org/apache/ws/security

-buildpath: \
  org.apache.ws.security:wss4j;version=${wss4jVersion}, \
  com.ibm.ws.org.apache.santuario.xmlsec.1.5.2, \
  com.ibm.ws.org.apache.commons.logging.1.0.3;version=latest, \
  com.ibm.ws.com.sun.xml.messaging.saaj;version=latest
