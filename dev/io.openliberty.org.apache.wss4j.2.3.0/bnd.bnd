-include= \
  jar:${fileuri;${repo;org.apache.cxf:wss4j-ws-security-common;2.3.0;EXACT}}!/META-INF/MANIFEST.MF, \
  jar:${fileuri;${repo;org.apache.wss4j:wss4j-bindings;2.3.0;EXACT}}!/META-INF/MANIFEST.MF, \
  jar:${fileuri;${repo;org.apache.cxf:wss4j-policy;2.3.0;EXACT}}!/META-INF/MANIFEST.MF, \
