-include= jar:${fileuri;${repo;org.apache.santuario:xmlsec;2.2.0;EXACT}}!/META-INF/MANIFEST.MF,bnd.overrides

santuarioVersion=2.2.0

globalize: false

-includeresource: \
  @${repo;org.apache.santuario:xmlsec;${santuarioVersion};EXACT}!/!META-INF/maven/*
